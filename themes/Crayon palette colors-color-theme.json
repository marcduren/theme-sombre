{
  "name": "Crayon palette colors",
  "colors": {
    "activityBarBadge.background": "#5a7ca4",
    "editor.background": "#1e1e1e",
    "editor.foreground": "#dddddd",
    "editor.lineHighlightBorder": "#444",
    "editor.selectionBackground": "#3a3a46",
    "editor.selectionHighlightBackground": "#ffff0020",
    "sideBarTitle.foreground": "#bbbbbb",
    "tab.activeBackground": "#2a2a55",
    "minimapSlider.activeBackground": "#8888bb55",
    "minimapSlider.background": "#8888bb55",
    "minimapSlider.hoverBackground": "#8888bb55"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#586270",
        "fontStyle": ""
      }
    },
    {
      "name": "Keywords + Types",
      "scope": ["keyword", "keyword.operator", "keyword.other", "storage.type", "support.type"],
      "settings": {
        "foreground": "#d39b9b",
        "fontStyle": ""
      }
    },
    {
      "scope": ["entity.name.function", "support.function.string.php meta.function-call"],
      "settings": {
        "foreground": "#a0c2da",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "variable.language", "support.variable"],
      "settings": {
        "foreground": "#ddd",
        "fontStyle": ""
      }
    },
    {
      "name": "Numbers, Characters, Constantes",
      "scope": ["string", "constant.character.escape", "string.regexp", "constant", "constant.numeric", "constant.character", "constant.language", "support.constant", "support.type"],
      "settings": {
        "foreground": "#dacba0",
        "fontStyle": ""
      }
    },
    {
      "name": "tag",
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#7fc6c6",
        "fontStyle": ""
      }
    },
    {
      "scope": ["entity.other.attribute-name.html"],
      "settings": {
        "foreground": "#95cbab"
      }
    },
    {
      "scope": ["support.type.property-name"],
      "settings": {
        "foreground": "#B27B78",
        "fontStyle": ""
      }
    },
    {
      "scope": ["source.vue meta.attribute.directive.vue entity.other.attribute-name.html"],
      "settings": {
        "fontStyle": "underline"

      }
    },
    {
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#383E47",
        "fontStyle": ""
      }
    },
    {
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#586270"
      }
    },
    {
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#CC99B3"
      }
    }
  ]
}

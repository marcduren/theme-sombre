{
  "name": "Crayon palette colors",
  "colors": {
    "editor.background": "#1e1e1e",
    "editor.foreground": "#d8d8d8",
    "editor.selectionBackground": "#383E47",
    "editor.selectionForeground": "#ddd",
    "editor.selectionHighlightBackground": "#ffff0020",
    "editor.lineHighlightBorder": "#444",
    "activityBarBadge.background": "#5a7ca4",
    "sideBarTitle.foreground": "#bbbbbb",
    "tab.activeBackground": "#404080"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#586270",
        "fontStyle": ""
      }
    },
    {
      "name": "Keywords + Types",
      "scope": ["keyword", "keyword.operator", "keyword.other", "storage.type", "support.type"],
      "settings": {
        "foreground": "#B39696",
        "fontStyle": ""
      }
    },
    {
      "scope": ["entity.name.function", "meta.function-call"],
      "settings": {
        "foreground": "#94a8c2",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "support.variable"],
      "settings": {
        "foreground": "#ddd",
        "fontStyle": ""
      }
    },
    {
      "name": "Numbers, Characters, Constantes",
      "scope": [
        "string",
        "constant.character.escape",
        "string.regexp",
        "constant",
        "constant.numeric",
        "constant.character",
        "constant.language",
        "support.constant",
        "variable.language",

        "support.type"
      ],
      "settings": {
        "foreground": "#b3bd95",
        "fontStyle": ""
      }
    },
    {
      "name": "tag",
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#69b2b2",
        "fontStyle": ""
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#9fc5af"
      }
    },
    {
      "scope": ["support.type.property-name"],
      "settings": {
        "foreground": "#B27B78",
        "fontStyle": ""
      }
    },
    {
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#383E47",
        "fontStyle": ""
      }
    },
    {
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#586270"
      }
    },
    {
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#CC99B3"
      }
    }
  ]
}
